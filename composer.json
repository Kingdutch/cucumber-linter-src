{
    "name": "kingdutch/cucumber-linter-src",
    "description": "Cucumber Linter - Static analysis tool for Cucumber and Behat",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "replace": {
        "kingdutch/cucumber-linter": "self.version"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CucumberLinter\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CucumberLinter\\Tests": [
                "tests/src/"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "bin/cucumber-linter"
    ]
}
